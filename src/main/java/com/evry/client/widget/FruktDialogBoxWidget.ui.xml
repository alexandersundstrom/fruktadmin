<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:u="urn:import:com.google.gwt.user.client.ui">
    <ui:style field="style" src="../css/dialogBox.css"/>

    <u:HTMLPanel>
        <div class="{style.fruktdialog}">
            <span class="{style.fruktdialog-title}">Title</span>
            <span>Description</span>
            <div class="{style.buttonContainer}">
                <u:Button ui:field="okButton">Ok</u:Button>
                <u:Button ui:field="cancelButton" addStyleNames="{style.cancel-button}">Cancel</u:Button>
            </div>
        </div>
    </u:HTMLPanel>

</ui:UiBinder>