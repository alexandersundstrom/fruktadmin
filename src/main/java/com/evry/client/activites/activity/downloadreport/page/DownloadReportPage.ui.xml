             xmlns:glass="urn:import:com.evry.client.activites.dependencies.widgets.glass"
             xmlns:table2="urn:import:com.evry.client.activites.activity.downloadreport.widget.table">

    <ui:style field="app" src="../../../dependencies/css/app.css" />
    <ui:style field="style" src="../../../dependencies/css/styling.css" />

    <g:HTMLPanel>
